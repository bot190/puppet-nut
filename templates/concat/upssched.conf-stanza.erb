AT <%= @notifytype %> <%= @real_upsname %> <%= @action -%>
<%- if @action == "START-TIMER" or @action == "CANCEL-TIMER" -%>
 <%= @timername -%>
<%- elsif @action == "EXECUTE" -%>
 <%= @command -%>
<%- end -%>
<%- if @action == "START-TIMER" -%>
 <%= @interval -%>
<%- elsif @action == "CANCEL-TIMER" and @command != nil -%>
 <%= @command -%>
<%- end %>